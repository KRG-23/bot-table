{
  "name": "munitorum",
  "version": "0.1.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"{src,scripts,docs}/**/*.{ts,tsx,js,json,md,yml,yaml}\"",
    "test": "npm run lint",
    "prepare": "husky install",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init"
  },
  "keywords": [
    "discord",
    "bot",
    "warhammer",
    "tables",
    "reservation"
  ],
  "author": "",
  "license": "MIT",
  "description": "Bot Discord de rÃ©servation de tables pour le club (Warhammer 40k/AoS/Kill Team).",
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "docs/**/*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "dayjs": "^1.11.19",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "pino": "^9.7.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prisma": "^6.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
